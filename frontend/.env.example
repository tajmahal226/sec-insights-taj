# Environment Variables for Frontend Deployment
# Copy this file to .env.local for local development

# Backend API URL (required)
# For local development: http://localhost:8000
# For production: Your Render backend URL (e.g., https://your-app.onrender.com)
NEXT_PUBLIC_BACKEND_URL=

# Node Environment (automatically set by Vercel)
NODE_ENV=production

# GitHub Codespaces (optional - automatically set in Codespaces)
CODESPACES=false
CODESPACE_NAME=

# Sentry Configuration (optional)
# NEXT_PUBLIC_SENTRY_DSN=
